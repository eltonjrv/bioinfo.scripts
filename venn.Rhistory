source("http://www.bioconductor.org/biocLite.R")
class(biocLite)
biocLite("limma")
library(limma)
hits = read.table('hits2venn.tab')
dim(hits)
attach(hits)
head(hits)
Lm_vs_Li = (hits[,2] >=1)
Lm_vs_Lmx = (hits[,3] >=1)
Lm_vs_Lb = (hits[,4] >=1)
ortho = cbind(Lm_vs_Li, Lm_vs_Lmx, Lm_vs_Lb)
head(ortho)
orthovenn = vennCounts(ortho)
vennDiagram(orthovenn)
