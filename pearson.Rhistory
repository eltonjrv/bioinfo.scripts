file = read.delim("pairs--aaID-vs-SAPF.tab")
m = as.matrix(file[,2:6])	#excluding the first column that is the GeneID (or any other feature name), and taking only the numerical columns
dim(m)
n = dim(m)
all_pcc = NULL
i=1
j=2
for(i in 1:(n-1)) { for(j in (i+1):n) { pcc_pair = cor(m[j,], m[i,]); all_pcc = c(all_pcc, pcc_pair)}}
mean(all_pcc)
